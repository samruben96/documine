# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# =============================================================================
# LLM Configuration (Story 5.10)
# =============================================================================

# LLM Provider - choose between OpenRouter (recommended) or direct OpenAI
# OpenRouter provides access to Claude, Gemini, GPT, and more via single API
# Options: openrouter | openai
LLM_PROVIDER=openrouter

# Chat Model - which model to use for chat responses
# Available via OpenRouter:
#   - claude-sonnet-4.5 (PRIMARY - best for insurance docs)
#   - claude-haiku-4.5 (fast, lower cost)
#   - gemini-2.5-flash (cost-optimized, 1M context)
#   - gpt-4o (fallback)
# Available via direct OpenAI:
#   - gpt-4o (only option)
LLM_CHAT_MODEL=claude-sonnet-4.5

# OpenRouter API Key (required when LLM_PROVIDER=openrouter)
# Get from https://openrouter.ai/keys
OPENROUTER_API_KEY=

# OpenAI API Key (always required for embeddings)
OPENAI_API_KEY=

# Embedding Model Configuration (uses OpenAI directly)
# Options: text-embedding-3-small | text-embedding-3-large
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Embedding Dimensions (must match vector column in database)
# text-embedding-3-small supports: 512, 1536
# text-embedding-3-large supports: 256, 1024, 3072
# WARNING: Changing this requires re-embedding all documents
OPENAI_EMBEDDING_DIMS=1536

# A/B Testing (optional)
# Enable to split users between default model and test model
AB_TEST_ENABLED=false
# Model to use for test group (50% of users when enabled)
AB_TEST_MODEL=claude-haiku-4.5

# Cohere (for RAG reranking - Story 5.8)
# Get key from https://dashboard.cohere.com/api-keys
# Cost: ~$1/1000 searches
COHERE_API_KEY=

# Document Parsing (Docling - self-hosted)
# Local development: http://localhost:8000
# Production: https://your-docling-service.railway.app
DOCLING_SERVICE_URL=http://localhost:8000

# DEPRECATED: LlamaParse replaced by Docling in Story 4.8
LLAMA_CLOUD_API_KEY=

# Email (Resend)
RESEND_API_KEY=

# App URL (for password reset links)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# =============================================================================
# AI Quoting - Skyvern Agent (Story Q6.2)
# =============================================================================

# Skyvern API Key (required for AI-powered quote automation)
# Get from https://app.skyvern.com/settings/api-keys
SKYVERN_API_KEY=

# Skyvern API Base URL (optional, defaults to https://api.skyvern.com/v1)
# Override for self-hosted Skyvern or development environments
SKYVERN_URL=https://api.skyvern.com/v1

# =============================================================================
# AI Quoting - Browser Use Agent (Story Q7.1)
# =============================================================================

# Anthropic API Key (required for Browser Use with Claude)
# Get from https://console.anthropic.com/settings/keys
# Note: This is separate from OpenRouter - Browser Use requires direct Anthropic API
ANTHROPIC_API_KEY=

# Browser Use Configuration
# Model to use for browser automation
# Options: claude-sonnet-4-5 (best), claude-sonnet-4-0 (good), claude-haiku-4-5 (fast)
BROWSER_USE_MODEL=claude-sonnet-4-5

# Headless mode (true for production, false for debugging/watching)
BROWSER_USE_HEADLESS=false

# =============================================================================
# CAPTCHA Solving - CapSolver (Story Q7.3)
# =============================================================================

# CapSolver API Key (required for automated CAPTCHA solving)
# Get from https://dashboard.capsolver.com/
# Supports: reCAPTCHA v2/v3, Cloudflare Turnstile, hCaptcha
# Cost: ~$0.50-$2.00 per 1000 CAPTCHAs depending on type
CAPSOLVER_API_KEY=
